<div class="mb-5">
    <table class="table table-striped table-hover">
        <thead>
        <tr>
            {% for header in activeFileHeaders %}
                <th>{{ header }}</th>
            {% endfor %}
        </tr>
        </thead>
        <tbody>
        {% for row in activeFileArray %}
            <tr>
                {% for header in activeFileHeaders %}
                    <td>{{ row[header] }}</td>
                {% endfor %}
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
