<div class="mb-5">
    <p class="h5 pb-2">Stored CSV Files</p>
    <ul class="list-group">
        {% for f in filesList %}
            <a class="list-group-item {{ f == activeFileName? 'active' : '2' }}"
               href="/?{{ queryStringFileName }}={{ f }}">
                {{ f }}
            </a>
        {% endfor %}
    </ul>
</div>
